      "source": [
        "# Importing the 'random' module to generate random IDs\n",
        "import random\n",
        "\n",
        "# This is the Employee class that defines the structure of an Employee object\n",
        "class Employee:\n",
        "    # The __init__ method is the initializer that is called when an instance of the class is created\n",
        "    def __init__(self, id, name, base_salary):\n",
        "        # The 'self' parameter refers to the instance of the class\n",
        "        self.id = id  # Unique identifier for the employee\n",
        "        self.name = name  # Name of the employee\n",
        "        self.base_salary = base_salary  # Base salary of the employee\n",
        "        self.performance = []  # List to store performance scores of the employee\n",
        "\n",
        "# This is the Employee Management System class that defines the structure of the Employee Management System\n",
        "class EmployeeManagementSystem:\n",
        "    # Initializer method\n",
        "    def __init__(self):\n",
        "        self.employees = []  # List to store all employee objects\n",
        "        self.id_counter = 0  # Counter to keep track of ordered IDs\n",
        "\n",
        "    # Method to add a single employee\n",
        "    def add_employee(self, id, name, base_salary):\n",
        "        # Creating a new employee object and appending it to the employees list\n",
        "        new_employee = Employee(id, name, base_salary)\n",
        "        self.employees.append(new_employee)\n",
        "        # Print message indicating successful addition of employee\n",
        "        print(f\"Employee added with ID: {id}, Name: {name}, Base Salary: {base_salary}\")\n",
        "\n",
        "    # Method to add multiple employees\n",
        "    def bulk_add_employee(self):\n",
        "        # Loop to continuously ask for employee details until 'exit' is entered\n",
        "        while True:\n",
        "            # Asking for employee details\n",
        "            details = input(\"Enter employee details (name, id and base_salary) in the format 'name,id,base_salary'. Type 'exit' to return to main menu:\\n\")\n",
        "            # If 'exit' is entered, break out of the loop\n",
        "            if details.lower() == 'exit':\n",
        "                break\n",
        "            # If 'exit' is not entered, attempt to split the input into name, id, and base_salary\n",
        "            try:\n",
        "                name, id, base_salary = details.split(',')\n",
        "                id = id.strip()\n",
        "                # If 'random_id' is entered, generate a random id using the 'random' module\n",
        "                if id.lower() == 'random_id':\n",
        "                    id = random.randint(1, 10000)\n",
        "                # If 'ordered_id' is entered, use the id_counter for the id and then increment the counter\n",
        "                elif id.lower() == 'ordered_id':\n",
        "                    id = self.id_counter\n",
        "                    self.id_counter += 1\n",
        "                # If a specific number is entered, use it as the id\n",
        "                else:\n",
        "                    id = int(id)\n",
        "                # Convert base_salary to a float\n",
        "                base_salary = float(base_salary)\n",
        "                # Use the add_employee method to add the new employee\n",
        "                self.add_employee(id, name, base_salary)\n",
        "            # If the input is in an invalid format, print an error message\n",
        "            except ValueError:\n",
        "                print(\"Invalid input. Please try again.\")\n",
        "\n",
        "    # Method to remove an employee\n",
        "    def remove_employee(self, employee_id):\n",
        "        # Loop through the list of employees\n",
        "        for i, employee in enumerate(self.employees):\n",
        "            # If the id of the current employee matches the given id\n",
        "            if employee_id == employee.id:\n",
        "                # Remove the employee from the list\n",
        "                del self.employees[i]\n",
        "                # Print a message indicating successful removal of employee\n",
        "                print(f\"Employee with ID: {employee_id} has been removed.\")\n",
        "                break\n",
        "\n",
        "    # Method to add a performance score to an employee\n",
        "    def add_performance(self, employee_id, score):\n",
        "        # Loop through the list of employees\n",
        "        for employee in self.employees:\n",
        "            # If the id of the current employee matches the given id\n",
        "            if employee_id == employee.id:\n",
        "                # Add the performance score to the employee's list of scores\n",
        "                employee.performance.append(score)\n",
        "                # Print a message indicating successful addition of performance score\n",
        "                print(f\"Performance score of {score} added to Employee with ID: {employee_id}.\")\n",
        "                break\n",
        "\n",
        "    # Method to calculate the salary of an employee\n",
        "    def calculate_salary(self, employee_id):\n",
        "        # Loop through the list of employees\n",
        "        for employee in self.employees:\n",
        "            # If the id of the current employee matches the given id\n",
        "            if employee_id == employee.id:\n",
        "                # If the employee has performance scores\n",
        "                if employee.performance:\n",
        "                    # Calculate the average performance score\n",
        "                    average_performance = sum(employee.performance) / len(employee.performance)\n",
        "                    # Calculate the salary using the formula given\n",
        "                    salary = employee.base_salary * (1 + (average_performance / 100))\n",
        "                else:\n",
        "                    # If the employee does not have performance scores, the salary is just the base salary\n",
        "                    salary = employee.base_salary\n",
        "                # Print the calculated salary\n",
        "                print(f\"Calculated salary for Employee with ID: {employee_id} is {salary}.\")\n",
        "\n",
        "    # Method to display the details of an employee\n",
        "    def view_employee_details(self, employee_id):\n",
        "        # Loop through the list of employees\n",
        "        for employee in self.employees:\n",
        "            # If the id of the current employee matches the given id\n",
        "            if employee_id == employee.id:\n",
        "                # Print the details of the employee\n",
        "                print(f\"ID: {employee.id}, Name: {employee.name}, Base Salary: {employee.base_salary}, Performance: {employee.performance}\")\n",
        "                break\n",
        "\n",
        "    # Method to display the details of all employees\n",
        "    def view_all_employees(self, password):\n",
        "        # If the correct password is entered\n",
        "        if password == 'password123':\n",
        "            # Loop through the list of employees\n",
        "            for employee in self.employees:\n",
        "                # Print the details of each employee\n",
        "                print(f\"ID: {employee.id}, Name: {employee.name}, Base Salary: {employee.base_salary}, Performance: {employee.performance}\")\n",
        "        else:\n",
        "            print(\"Incorrect password. Access denied.\")\n",
        "\n",
        "# Instantiating the Employee Management System\n",
        "ems = EmployeeManagementSystem()\n",
        "\n",
        "# While loop to continuously display the main menu and ask for user input\n",
        "while True:\n",
        "    print(\"1. Add employee\")\n",
        "    print(\"2. Add multiple employees\")\n",
        "    print(\"3. Remove employee\")\n",
        "    print(\"4. Add performance score\")\n",
        "    print(\"5. Calculate salary\")\n",
        "    print(\"6. View employee details\")\n",
        "    print(\"7. View all employees\")\n",
        "    print(\"8. Exit\")\n",
        "    choice = input(\"Enter your choice: \")\n",
        "\n",
        "    if choice == '1':\n",
        "        id = input(\"Enter the ID for the new employee (Enter 'random_id' for random ID, 'ordered_id' for the next ID in order): \")\n",
        "        if id.lower() == 'random_id':\n",
        "            id = random.randint(1, 10000)\n",
        "        elif id.lower() == 'ordered_id':\n",
        "            id = self.id_counter\n",
        "            self.id_counter += 1\n",
        "        else:\n",
        "            id = int(id)\n",
        "        name = input(\"Enter the name for the new employee: \")\n",
        "        base_salary = float(input(\"Enter the base salary for the new employee: \"))\n",
        "        ems.add_employee(id, name, base_salary)\n",
        "    elif choice == '2':\n",
        "        ems.bulk_add_employee()\n",
        "    elif choice == '3':\n",
        "        id = int(input(\"Enter the ID of the employee to remove: \"))\n",
        "        ems.remove_employee(id)\n",
        "    elif choice == '4':\n",
        "        id = int(input(\"Enter the ID of the employee to add a performance score to: \"))\n",
        "        score = float(input(\"Enter the performance score (0-100): \"))\n",
        "        ems.add_performance(id, score)\n",
        "    elif choice == '5':\n",
        "        id = int(input(\"Enter the ID of the employee to calculate the salary of: \"))\n",
        "        ems.calculate_salary(id)\n",
        "    elif choice == '6':\n",
        "        id = int(input(\"Enter the ID of the employee to view: \"))\n",
        "        ems.view_employee_details(id)\n",
        "    elif choice == '7':\n",
        "        password = input(\"Enter the password to view all employees: \")\n",
        "        ems.view_all_employees(password)\n",
        "    elif choice == '8':\n",
        "        break\n",
        "    else:\n",
        "        print(\"Invalid choice. Please try again.\")\n"
      ]
    }
